        "AHTN_CSV_URL",
        "AHTN_SOURCE_URL",
        "API_KEY_PEPPER",
        "API_URL",
        "APPDATA",
        "AWS_REGION",
        "BATCH_SIZE",
        "BETTER_AUTH_URL",
        "CLEARCOST_ADMIN_API_KEY",
        "CLEARCOST_API_KEY",
        "CLEARCOST_API_URL",
        "CLEARCOST_BASE_URL",
        "CLEARCOST_TASKS_API_KEY",
        "CLEARCOST_WEBHOOK_TIMEOUT_MS",
        "CODECOV_TOKEN",
        "COMMIT_SHA",
        "CONTACT_WEBHOOK_URL",
        "CURRENCY_BASE",
        "DATA_REMOTE_BASE",
        "DATABASE_URL",
        "DB_DATA_API_BASE",
        "DISCORD_WEBHOOK_URL",
        "EMAIL_OTP_API_SECRET",
        "EU_TARIC_COMPONENT_URL",
        "EU_TARIC_DUTY_EXPR_URL",
        "EU_TARIC_GEO_DESC_URL",
