        "UPSTASH_REDIS_REST_URL",
        "US_HMF_EFFECTIVE",
        "US_HMF_NOTES",
        "US_HMF_PCT",
        "US_MPF_EFFECTIVE",
        "US_MPF_MAX",
        "US_MPF_MIN",
        "US_MPF_NOTES",
        "US_MPF_PCT",
        "VERCEL",
        "VERCEL_BRANCH_URL",
        "VERCEL_ENV",
        "VERCEL_GIT_COMMIT_SHA",
        "VERCEL_PROJECT_PRODUCTION_URL",
        "VERCEL_URL",
        "WEB_ORIGIN",
        "WEBHOOK_KMS_KEY"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "outputs": []
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ],
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
