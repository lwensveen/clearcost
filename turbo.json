        "EU_TARIC_LANGUAGE",
        "EU_TARIC_MEASURE_URL",
        "EU_TARIC_REMEDY_TYPES",
        "FX_MAX_AGE_HOURS",
        "FX_SECONDARY",
        "FX_SECONDARY_MAX_LAG_DAYS",
        "HTS_API_BASE",
        "IMPORT_AGE_BUCKETS",
        "IMPORTS_SWEEP_THRESHOLD_MINUTES",
        "IMPORT_STALE_MINUTES",
        "INTERNAL_SIGNING_SECRET",
        "KV_REST_API_TOKEN",
        "KV_REST_API_URL",
        "NEXT_PUBLIC_API_BASE",
        "NEXT_PUBLIC_CLEARCOST_API_URL",
        "NEXT_SERVER_ACTIONS_ENCRYPTION_KEY",
        "NODE_ENV",
        "OXR_APP_ID",
        "PORT",
        "RATE_LIMIT_MAX",
        "RATE_LIMIT_WINDOW",
